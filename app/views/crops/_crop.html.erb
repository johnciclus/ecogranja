			  <td><b>Potasio extraido</b></td>
			  <td>	 <%= @crop.KExtraPro %></td>
		</tr>
		<tr>	  
			  <td><b>Promedio Ph </b></td>
			  <td>	 <%= @crop.ProPhPlantas %></td>
		</tr>
		<tr>	  
			  <td><b>Desviación estandar del Ph</b></td>
			  <td>	 <%= @crop.DesPhPlantas %></td>
		</tr>
		<tr>	  
			  <td><b>Peso seco de una planta</b></td>
			  <td>	 <%= @crop.PesoUniCultivo %></td>
		</tr>
		<tr>  
			  <td><b>Desviación estandar del peso seco de una planta</b></td>
			  <td>	 <%= @crop.DesPesoUniCul %></td>
		</tr>
		<tr>  
			  <td><b>Peso seco de la cosecha</b></td>
			  <td>	 <%= @crop.PesoUniCosecha %></td>
