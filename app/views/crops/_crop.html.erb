<% unless @crop.nil? %>
	<table id="tablaCultivo">
	<thead>
	<tr>
		</tr>
			<th>Propiedad</th>
			<th>Valor</th>
		<tr>
	  
	</tr>
	</thead>
	<tbody>	
		<tr>
		  	  <td><b>Articulo</b></td>
			  <td><b><%= @crop.article.title %></b></td>
		</tr>
		<tr>
			  <td><b>Nombre del cultivo</b></td>
			  <td>	 <%= @crop.name %></td>
		</tr>
		<tr>	  
			  <td><b>Descripción del cultivo</b></td>
			  <td>	 <%= @crop.description %></td>
		</tr>
		<tr>	  
			  <td><b>Nitrogeno absorbido</b></td>
			  <td>	 <%= @crop.NabsorPro %></td>
		</tr>
		<tr>	  
			  <td><b>Fósforo absorbido</b></td>
			  <td>	 <%= @crop.PAbsorPro %></td>
		</tr>
		<tr>	  
			  <td><b>Potasio absorbido</b></td>
			  <td>	 <%= @crop.KAbsorPro %></td>
		</tr>
		<tr>	  
			  <td><b>Nitrógeno extraido</b></td>
			  <td>	 <%= @crop.NExtraPro %></td>
