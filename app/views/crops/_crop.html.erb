			  <td><b>Tiempo de desarrollo</b></td>
			  <td>	 <%= @crop.TiemVegetativo %></td>
		</tr>
		<tr>	  
			  <td><b>Tiempo productivo</b></td>
			  <td>	 <%= @crop.TiemReproductivo %></td>
		</tr>
		<tr>	  
			  <td><b>Luminosidad promedio del cultivo</b></td>
			  <td>	 <%= @crop.ProLumiPlanta %></td>
		</tr>
		<tr>	  
			  <td><b>Desviación estandar de la Luminosidad</b></td>
			  <td>	 <%= @crop.DesLumiPlanta %></td>
		</tr>
